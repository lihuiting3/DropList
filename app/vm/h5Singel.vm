     <div class="cms-single" style="
    background: #fff;">
        #if($!{cms_module_title}&&$!{cms_module_title}!="")
                <div class="title">
                    <h2>$!{cms_module_title}</h2>
                </div>
                <span class="span"></span>
        #end
        <div class="cms-module-fillprice">
        #foreach($item in $submodule1)
